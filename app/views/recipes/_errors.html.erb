<% if shared.errors.any? %>
<div id="error_explanation">
    <h2>
        <%= pluralize(shared.errors.count, "error") %>
        prohibited this shared from being saved:
    </h2>

    <ul>
        <% shared.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
        <% end %>
    </ul>
</div>
<% end %>